echo "\
super + enter                     open terminal
super + q                         close window
super + shift + q                 close application
super + m                         toggle fullscreen
super + left click + move         move floating window:
super + right click + move        resize floating window:
super + shift + r                 reload config:
super + {h,j,k,l}                 navigate
super + shift + right             rotate 90 to right
super + shift + left              rotate 90 to left
super + shift + {h,j,k,l}         switch window places
super + alt + {h,j,k,l}           resize window expand
super + alt + shift + {h,j,k,l}   resize window contract
super + {0-9}                     change workspace
super + shift + {0-9}             move window to workspace
super + t                         tiled mode
super + shift + t                 pseudo-tiled mode
super + s                         floating mode
super + f                         fullscreen mode
super + g                         switch places with biggest window
super + tilde                     alternate window focus
super + tab                       last workspace focus
super + ctrl + {h,j,k,l}          preselect direction
super + ctrl + {1-9}              preselect ratio
super + shift + e                 exit xorg

# rofi
super + d                         open rofi menu
super + shift + d                 open rofi tabs menu

# rofi selection
tab         next
ctrl + n    next
ctrl + p    previous
"